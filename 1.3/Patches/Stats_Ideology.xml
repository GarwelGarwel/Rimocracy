				<xpath>/Defs/StatDef[defName="ConversionPower"]/parts</xpath>
				<value>
					<li Class="Rimocracy.StatPart_StateIdeologion">
						<factor>1.25</factor>
					</li>
				</value>
			</li>
		</operations>
	</Operation>

</Patch>
