<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<StatDef>
		<defName>GovernEfficiencyFactor</defName>
		<label>govern efficiency factor</label>
		<description>The speed at which the leader will improve governance is multiplied by this value.</description>
		<category>Building</category>
		<defaultBaseValue>1</defaultBaseValue>
		<minValue>0</minValue>
		<toStringStyle>PercentZero</toStringStyle>
		<showIfUndefined>false</showIfUndefined>
		<parts>
			<li Class="StatPart_WorkTableTemperature" />
			<li Class="StatPart_RoomStat">
				<roomStat>GovernEfficiencyFactor</roomStat>
				<customLabel>Room impressiveness</customLabel>
			</li>
			<li Class="StatPart_Quality">
				<factorAwful>0.80</factorAwful>
				<factorPoor>0.90</factorPoor>
				<factorGood>1.05</factorGood>
				<factorExcellent>1.10</factorExcellent>
				<factorMasterwork>1.15</factorMasterwork>
				<factorLegendary>1.20</factorLegendary>
			</li>
		</parts>
		<displayPriorityInCategory>1000</displayPriorityInCategory>
	</StatDef>

</Defs>
